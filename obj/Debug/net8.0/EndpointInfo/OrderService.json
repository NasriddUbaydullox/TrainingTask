{
  "openapi": "3.0.4",
  "info": {
    "title": "Order Service API",
    "description": "Very Good",
    "version": "v1"
  },
  "paths": {
    "/api/Order/{id}": {
      "get": {
        "tags": [
          "Order"
        ],
        "summary": "Get Product By Id",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Product found",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/OrderDto"
                },
                "example": {"id":1,"productId":8,"createdAt":"2025-06-15T17:48:34.4172272Z","quantity":10}
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/OrderDto"
                },
                "example": {"id":1,"productId":8,"createdAt":"2025-06-15T17:48:34.4172272Z","quantity":10}
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/OrderDto"
                },
                "example": {"id":1,"productId":8,"createdAt":"2025-06-15T17:48:34.4172272Z","quantity":10}
              }
            }
          },
          "404": {
            "description": "Product not found"
          }
        }
      }
    },
    "/api/Order": {
      "post": {
        "tags": [
          "Order"
        ],
        "summary": "Create Product",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateOrderCommand"
              },
              "example": {"productId":1,"createdAt":"2025-06-15T17:48:34.4235062Z","quantity":10}
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateOrderCommand"
              },
              "example": {"productId":1,"createdAt":"2025-06-15T17:48:34.4235062Z","quantity":10}
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/CreateOrderCommand"
              },
              "example": {"productId":1,"createdAt":"2025-06-15T17:48:34.4235062Z","quantity":10}
            }
          }
        },
        "responses": {
          "200": {
            "description": "Product created",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/OrderDto"
                },
                "example": {"id":1,"productId":8,"createdAt":"2025-06-15T17:48:34.4235994Z","quantity":10}
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/OrderDto"
                },
                "example": {"id":1,"productId":8,"createdAt":"2025-06-15T17:48:34.4235994Z","quantity":10}
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/OrderDto"
                },
                "example": {"id":1,"productId":8,"createdAt":"2025-06-15T17:48:34.4235994Z","quantity":10}
              }
            }
          },
          "400": {
            "description": "Invalid product data"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "CreateOrderCommand": {
        "type": "object",
        "properties": {
          "dto": {
            "$ref": "#/components/schemas/CreateOrderDto"
          }
        },
        "additionalProperties": false
      },
      "CreateOrderDto": {
        "type": "object",
        "properties": {
          "productId": {
            "type": "integer",
            "format": "int32"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "quantity": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "OrderDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "productId": {
            "type": "integer",
            "format": "int32"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "quantity": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      }
    }
  }
}